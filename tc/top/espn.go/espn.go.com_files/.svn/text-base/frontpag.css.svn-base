/* Spotlight */
#spotlight {
	margin: 0; 
	padding: 0 0 10px 7px;
}
/* end Spotlight */


/* Must read */
ul#must-read {
	border-bottom: solid 1px #bab8a9;
	list-style: none;
    margin: 0 7px 10px 7px;
    padding: 0 0 10px 0;
}
ul#must-read li {
    margin: 0 0 10px 0;
    overflow: hidden;
    padding: 0;
}
ul#must-read li p {
    margin: 0;
    padding: 0 0 0 4px;
}
ul#must-read h1 {
    background: #d1cdae;
    color: #000; 
    font-family: Verdana, sans-serif;
    font-size: 100%;
    margin: 0 0 10px 0;
    padding: 3px 0 3px 5px;
}
ul#must-read h1 em {
   float: right;
   font-style: normal;
   font-weight: normal;
   padding-right: 7px;
}
ul#must-read h1 em a:link, ul#must-read h1 em a:link:visited{color: #37351c;}
ul#must-read h1 em a:link:hover, ul#must-read h1 em a:link:visited:hover {color: #000;}

.must-photo {
	float: left;
    margin: 0 10px 0 0;
    padding: 0;
}
/* end Must read */


/* Ombudsman */
dl#ombuds {    
	background: #d1cdae;
	font-size: 110%;
	list-style: circle;
	margin: 0 7px 10px 7px;
	padding: 0 0 15px 0;
}

dl#ombuds dt {
	background: #a1a487;
	color: #fff;
	font-weight: bold;
	margin: 0 0 7px 0;
	padding: 3px 3px 3px 7px;
}

dl#ombuds dt em {
	display: inline;
	float: right;
	font-style: normal;
	font-weight: normal;
	padding-right: 7px;
}

dl#ombuds dt em a:link, dl#ombuds dt em a:link:visited {color: #fff;}

dl#ombuds dd {
	margin: 0;
	padding: 0 7px;
}
/* end Ombudsman */

