	line-height: 1px;
	height: 9px;
	width: 185px;
}

.aw_nav li ul.aw_drop li a {
	color: black;
	line-height: 18px;
	padding: 0 8px;
	width: 185px;
}


/* for multi links, ie "Stats | Hollinger Stats" */
.aw_drop li.multi {
	padding: 0 8px;
	width: 100%;
}

.aw_nav li .aw_drop li.multi a {
