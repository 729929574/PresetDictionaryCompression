/* Page structure */
body {
	background: #4b4b4b url(bg_main_.gif) repeat-y fixed 50% 50%;
	color: #000; 
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	font-size: x-small;
	line-height: 1.5em;  
	margin: 0; 
	padding: 0; 
	text-align: left;
}

a:link {color: #000;}
a:visited {color: #666;}
a:link:hover, a:visited:hover {color: #f00;}

.clear {clear: both;}

#container {
	background: #fff url(bg_conte.png) repeat-y;
	border: solid #373737;
    border-width: 0 1px;
	margin: auto;
	padding: 0;
	width: 990px;
}
#main { 
	margin: 0;
	padding: 0;
}
/* end Page structure */

/******* LEFT COLUMN ********/
#leftc {
	float: left;
	margin: 0;  
	padding: 0;
	width: 424px; 
}

/* Top Story */
#topstory {
	padding:0px 0px 7px 0px;
}
#topstory h1.head40 {
	font-family:Helvetica,Arial,sans-serif;
	font-size:40px;
	font-weight:bold;
	line-height:36px;
	margin:0pt;
	padding: 18px 0px 0px 7px
}
#topstory h1.head40 a, #topstory h1.head40 a:hover, #topstory h1.head40 a:visited, #topstory h1.head40 a:visited:hover {
    color: #000;
    text-decoration: none;
}
#topstory h1.head60 {
	font-family: Helvetica, Arial, sans-serif; 
	font-size: 60px; 
	font-weight: bold;
	line-height: 60px;	
	margin: 0;
	padding: 7px 0 6px 7px; 
}
#topstory h1.head60 a, #topstory h1.head60 a:hover, #topstory h1.head60 a:visited, #topstory h1.head60 a:visited:hover {
    color: #000;
    text-decoration: none;
}

#topstory img.ts-photo {
	margin:0pt;
	padding:7px 0px 0px 7px;
}
#topstory ul {
	list-style-image:none;
	list-style-position:outside;
	list-style-type:none;
	margin:0px;
	padding:0px 0px 13px;
}
#topstory ul li.photocap {
	background: #d9d9b7;
	color: #666; 
	font-weight: bold; 
	margin: 0 5px 2px 7px;
	padding: 0 0 6px 4px;
}
#topstory ul li.photocap em {
	color: #939374; 
	display: block;
	font-size: 90%; 
	font-style: normal;
	font-weight: normal; 
	margin: 0;
	padding: 1px 3px 1px 0; 
	text-align: right;
}
#topstory ul li.topstory2 {
	font-size: 110%; 
	margin: 0;
	padding:4px 10px 0 7px;
}
/* end Top Story */


/* WAR Large */
#warlrg {
	background: #fff;
	border-bottom: 1px solid #cdcdcd;
	color: #fff; 
	float: left;
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	font-size: 11px; 
	height: 345px; 
	line-height: 13px; 
	margin: 0;
	overflow: hidden;
	padding: 7px;
	width: 976px;
}

#warlrg a, #warlrg2 a:visited {
	color: #fff;
	text-decoration: underline;
}
#warlrg a:hover, #warlrg2 a:visited:hover {color: #e7a500;}

#warlrg .content {
	height: 136px;
	position: relative;
	width: 976px;
}
#warlrg .content .superheadline {
	left: 5px;
	position: absolute; 
	top: 0;
	z-index: 10000;
}
#warlrg .content .subheadline { 
	font-weight: bold;
	left: 10px;
	margin: 0;
	padding: 0;
	position: absolute; 
	top: 100px; 
	width: 460px; 
}
#warlrg .content .subheadline a, #warlrg2 .content .subheadline a:visited {
	color: #e7a500;
	text-decoration: none;
}
#warlrg .content .subheadline a:hover, #warlrg2 .content .subheadline a:visited:hover {text-decoration: underline;}

#warlrg .content .bullets {
	left: 515px;
	margin: 0;
	padding: 0;
	position: absolute;  
	top: 15px; 
	width: 455px;
}
/* end War Large */


