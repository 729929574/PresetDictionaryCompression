.RightCol { width: 460px; }

span.Explain { font-weight: normal; font-size: 12px; color: #000; }



#homepage_ad {text-align:center;width:300px;margin-left:15px;margin-bottom:20px;}

/* video group promo */
div#vid_grouppromo {
	margin: -10px 0 0 1px;
}
div#vid_grouppromo a:hover {
	background-color: #fff;
}