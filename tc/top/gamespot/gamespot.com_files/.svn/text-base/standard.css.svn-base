/* SKY COL WRAP */
#sky_col_wrap {
width:760px;
background:#3F4A59 url(skywrap_.gif) repeat-y;
border-top:1px solid #7C8796;
float:left;
}

#pc #sky_col_wrap {
background:#2B425E url(skywrap_.gif) repeat-y;
border-top:1px solid #70A1D3;
}

#playstation #sky_col_wrap {
background:#2A2E4F url(skywrap_.gif) repeat-y;
border-top:1px solid #649ACC;
}

#xbox #sky_col_wrap {
background:#37422D url(skywrap_.gif) repeat-y;
border-top:1px solid #96AE63;
}

#nintendo #sky_col_wrap {
background:#3B3747 url(skywrap_.gif) repeat-y;
border-top:1px solid #C197EA;
}

#portable #sky_col_wrap {
