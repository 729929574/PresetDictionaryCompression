#wiki #xbox #wiki_more_links .module {background:url(bottombg.gif) no-repeat scroll bottom;}
#wiki #pc #wiki_more_links .module {background:url(bottombg.gif) no-repeat scroll bottom;}
#wiki #playstation #wiki_more_links .module {background:url(bottombg.gif) no-repeat scroll bottom;}
#wiki #nintendo #wiki_more_links .module {background:url(bottombg.gif) no-repeat scroll bottom;}


