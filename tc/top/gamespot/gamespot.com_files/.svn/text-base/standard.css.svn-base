}

#xbox #sky_col_wrap #side_col_wrap {
background-image:url(side_sky.gif);
}

#nintendo #sky_col_wrap #side_col_wrap {
background-image:url(side_sky.gif);
}

#portable #sky_col_wrap #side_col_wrap {
