<script>
function gamehelp_selectTab(tab){

	document.getElementById('gamehelp_btn_guides').style.backgroundImage="url(http://media.ignimgs.com/media/ign/images/gamehelp_guides_btn_off.gif)";
	document.getElementById('gamehelp_btn_cheats').style.backgroundImage="url(http://media.ignimgs.com/media/ign/images/gamehelp_cheats_btn_off.gif)";
	document.getElementById('gamehelp_btn_faqs').style.backgroundImage="url(http://media.ignimgs.com/media/ign/images/gamehelp_faqs_btn_off.gif)";
	document.getElementById('gamehelp_btn_'+tab).style.backgroundImage="url(http://media.ignimgs.com/media/ign/images/gamehelp_"+tab+"_btn_on.gif)";
	
	document.getElementById('gamehelp_btn_guides').style.cursor="pointer";
	document.getElementById('gamehelp_btn_cheats').style.cursor="pointer";
	document.getElementById('gamehelp_btn_faqs').style.cursor="pointer";
	document.getElementById('gamehelp_btn_'+tab).style.cursor="default";
		
	document.getElementById('gh_guides').style.color='';
	document.getElementById('gh_cheats').style.color='';
	document.getElementById('gh_faqs').style.color='';
	document.getElementById('gh_'+tab).style.color="#fff";
	
	document.getElementById('gh_guides').style.cursor="pointer";
	document.getElementById('gh_cheats').style.cursor="pointer";
	document.getElementById('gh_faqs').style.cursor="pointer";
	document.getElementById('gh_'+tab).style.cursor="default";
	
	document.getElementById('gamehelp_guides').style.display="none";
	document.getElementById('gamehelp_cheats').style.display="none";
	document.getElementById('gamehelp_faqs').style.display="none";
	document.getElementById('gamehelp_'+tab).style.display="block";

	return trackclick('helplinks_hub_'+tab+'_tab-click.ign.com');


}
</script>





