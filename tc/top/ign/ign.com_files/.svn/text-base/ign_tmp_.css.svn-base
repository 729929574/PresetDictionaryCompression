#gamehelp_btn_cheats{width:97px;background-image:url(gamehelq.gif); }
#gamehelp_btn_faqs{width:97px;background-image:url(gamehelr.gif)}
.gamehelp_rank{color:#000;font-size:11px;font-weight:bold}
.gamehelp_btn_text{font-weight:bold;position:relative;top:6px;left:6px}
.gamehelp_more{font-weight:bold;text-align:right;margin-right:5px}
.gamehelpTitle,.gamehelpAll{font-weight:bold;font-size:11px}
.gamehelpAll {padding:3px 5px 3px 0}

/* featured ad unit */
#featuredAd { background-image:url(featured.gif); width:302px; height:130px; }
#featuredAdImage { padding:5px 10px 5px 17px; float:left; width:120px; }
#featuredAdText { padding:16px 0px 0px 0px; float:left; width:128px; }	

/************************
* poll
************************/
#poll { background-image:url(poll_bg0.gif); width:131px; float:right; }
.pollQuestions, .pollAnswers { width:111px; padding:0px 10px 10px 10px; text-align:left; }
.pollQuestions input { margin:2px; } 
.pollImage {margin-left:6px;}

/************************
* Jamdat Flash Game Finder (should only be on the Wireless channel)
************************/
#gameFinder{padding-top:10px}

/************************
* Google Adsense
************************/
#afcGoogleHub {background:url(colCente.gif) repeat-y;}
#afcGoogleHub #ad-hdr {background:url(editHead.gif) repeat-x #000;  margin: 0;height:30px;width: auto; text-align:left;color:#fff;font-weight:bold; font-size:17px;padding:10px 0px 0px 10px}
#afcGoogleHub #ad-wrap{height: auto; background: #e5ebf9; font: 11px arial,helvetica,sans-serif; overflow: hidden; margin: 0; padding:0; width:498px;border:0;}
#afcGoogleHub #ad-wrap a {text-align: left; text-decoration: none; display: block; width: 215px; margin: 0; padding: 5px 10px 10px 10px; float: left;}  
* html #afcGoogleHub#ad-wrap a { width: 225px;}  
#afcGoogleHub #ad-wrap p {color: #000; font-weight: normal; margin: 0; padding: 0;}  
#afcGoogleHub #ad-wrap span {color:#009; font-weight: normal; text-decoration: none; }  
#afcGoogleHub #ad-wrap strong {color:#009;font-size: 12px; font-weight: bold; text-decoration: underline; }
#afcGoogleHub #innerAfcGoogle {margin-left:5px;border:1px solid #000;width:498px}
#afcGoogleHub .afcRow {margin-top: -1px; padding:0; width:498px; height: auto;background:#F5F5F5 url(latest_v.gif) repeat-x; overflow: hidden;}  