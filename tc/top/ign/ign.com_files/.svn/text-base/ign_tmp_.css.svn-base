/*Latest Updates */
#latestUpdates{width:510px;text-align:left}
#latestUpdates_header,#latestUpdates_content {background:url(colCente.gif) repeat-y}
#latestUpdates_content{padding:0;}
#latestUpdates .header{position:relative;left:4px;font-size:17px;height:65px; font-weight:bold; border:0px solid black;color:#FFF;background-color:#000;}
#latestUpdates .header_divider{padding:0px 10px;height:32px;background:url(latest_u.gif) no-repeat}
#latestUpdates .header_divider_text{font-weight:bold; font-size:9px;font-family:tahoma,arial,helvetica,sans-serif;}
#latestUpdates .myDefaultCheck { width:14px; }
#latestUpdates .header_checkbox{padding-left:5px;}
#latestUpdates .header_divider form{padding:0px; margin:0px;}
#latestUpdates .tabHeaderText{padding:10px 0px 0px 10px;text-align:left;font-weight:bold;font-size: 17px}
#latestUpdates .tabContainer{position:absolute;bottom:-1px;left:0;right:0;font-size:11pt;color:#009;}
#latestUpdates .tab, #latestUpdates .tab_on {float:left;position:relative;height:27px}
#latestUpdates  .tab_text{font-weight:bold;font-size:12px;height:15px;position:relative;top:7px;}
.left_tab,.right_tab,.left_tab_on,.right_tab_on {position:absolute;float:left;height:27px;width:5px; }
.left_tab,.left_tab_on {left:0;}
.right_tab,.right_tab_on{right:0;}
.middle_tab,.middle_tab_on{float:left;padding:0 5px;height:27px;}
.left_tab {background:url(tab_left.gif) no-repeat}
.left_tab_on { background:url(tab_lefu.gif) no-repeat}
.middle_tab {background:url(tab_midd.gif) repeat-x;cursor:pointer}
.middle_tab_on {background:url(tab_mide.gif) repeat-x;text-decoration:none;color:#FFF;cursor:default}
.right_tab {background:url(tab_righ.gif) no-repeat}
.right_tab_on {background:url(tab_rigi.gif) no-repeat}

/*REMOVE THIS WHEN THE NEW IMAGE IS OFF THE MY UPDATES TAB*/
.right_tab_new,.right_tab_on_new{position:absolute;float:left;height:27px;width:30px;right:0;}
.right_tab_new {background:url(tab_rigj.gif) no-repeat;cursor:pointer}
.right_tab_on_new {background:url(tab_rigj.gif) no-repeat}
.middle_tab_new,.middle_tab_on_new{float:left;padding:0 30px 0 5px;height:27px;}
.middle_tab_new {background:url(tab_midd.gif) repeat-x;cursor:pointer}
.middle_tab_on_new {background:url(tab_mide.gif) repeat-x;text-decoration:none;color:#FFF;cursor:default}
