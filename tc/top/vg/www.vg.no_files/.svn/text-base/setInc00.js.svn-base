		settFile.type='text/javascript';
		settFile.src='/js/sett.js';
		// settFile.src='http://dev.kroma.no/scroll/sett.js';
		settFile.id='Kroma';

		document.body.appendChild(settFile);
	}
}

settIncSelector();
