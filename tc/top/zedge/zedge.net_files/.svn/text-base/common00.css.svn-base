.subnotice				{ color: #333; }

.larger					{ font-size: larger; }
.smaller				{ font-size: .9em; }

.spacer					{ background-color: #fff; border-right: 1px solid #ccc; }

/* NEEDS TO BE MORE SPESIFIC */
